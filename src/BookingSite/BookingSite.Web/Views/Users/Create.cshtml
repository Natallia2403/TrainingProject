@model BookingSite.Web.ViewModels.CreateUserViewModel
@{
    ViewBag.Title = "Добавление пользователя";
}
<form asp-action="Create" asp-controller="Users">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div>
        <label asp-for="Email">Email</label>
        <input type="text" asp-for="Email" />
    </div>
    <div>
        <label asp-for="Password">Пароль</label>
        <input type="password" asp-for="Password" />
    </div>
    <div>
        <label asp-for="Year">Год рождения</label>
        <input type="number" asp-for="Year" />
    </div>
    <div>
        <input type="submit" value="Добавить" class="btn btn-outline-secondary" />
    </div>
</form>